#include <stdio.h>
#include "math.h"

int main()
{
	printf("Add : %x\n",i_add(0x0FFF1111,0xAAAA5555));

	printf("mul : %llx\n",p_mul(0x0FFF1111,0x0FFF1111 ));

	printf("mul : %I64x\n",p_mul(0x0FFF1111,0xAAAA5555));

	printf("div : %f\n",i_div(100,8));


	return 0;
}
